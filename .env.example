# Flask Configuration
# Copy this file to .env and update with your actual values

# Flask Secret Key - Generate a random string for production
# You can generate one using: python -c "import os; print(os.urandom(24).hex())"
SECRET_KEY=your-secret-key-here-change-this-in-production

# Debug Mode - Set to False in production
DEBUG=True

# Flask App Port
PORT=2500

# Upload Directory - Relative to project root
# Default: static/image
IMAGE_UPLOADS=static/image

# Model Path - Relative to project root
# Default: SkinCancerClassificationModelFinal.h5
MODEL_PATH=SkinCancerClassificationModelFinal.h5

# Model Configuration
IMG_WIDTH=28
IMG_HEIGHT=28
BATCH_SIZE=10

# Flask App Host
HOST=0.0.0.0
